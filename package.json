{
  "name": "malweb",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@artsy/fresnel": "^3.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^2.1.1"
  },
  "scripts": {
    "predeploy": "npm run build && echo 'mal-lang.org' >build/CNAME && cp -fp build/index.html build/404.html",
    "deploy": "gh-pages --dist build --branch master --message Deploy --no-history",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --passWithNoTests",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@semantic-ui-react/craco-less": "^3.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "2.5.1"
  }
}
